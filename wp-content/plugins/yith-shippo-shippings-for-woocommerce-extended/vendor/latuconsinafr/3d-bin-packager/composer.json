{
    "name": "latuconsinafr/3d-bin-packager",
    "description": "PHP Library for 3D Bin Packing.",
    "keywords": [
        "php",
        "bin-packing",
        "2d-bin-packing",
        "3d-bin-packing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Farista Latuconsina",
            "email": "faristalatuconsina@gmail.com",
            "homepage": "https://github.com/latuconsinafr"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Latuconsinafr\\BinPackager\\BinPackager3D\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit --testdox --configuration phpunit.xml --do-not-cache-result --colors=always",
        "test:coverage": "php -d xdebug.mode=coverage -r \"require 'vendor/bin/phpunit';\" -- --configuration phpunit.xml --do-not-cache-result --colors=always --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    }
}